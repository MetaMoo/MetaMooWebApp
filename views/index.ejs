<!DOCTYPE html>
<html>
  <head>
  	<% include ./partials/head %>
  </head>
  <body>

    <header>
     <!-- <% include ./partials/header %> -->
    </header>

    <main>
      <div class="container">
        <div class="row">
        <!-- Metamoo logo -->
          <div class="col-md-4"></div>
          <div class="col-md-4">
            <img src="/images/main_logo.png" class="img-responsive text-center" alt="Responsive image">
          </div>
          <div class="col-md-4"></div>
        </div>

       
        <div class = "row">
        <!-- For search bar and searh button -->
          <div class="row search">
            <div class="col-sm-8 col-sm-offset-2">
              <form role="form">
                <div class="input-group">
                  <input type="text" class="form-control input-sm">
                  <span class="input-group-btn">
                    <button class="btn btn-default btn-sm" type="submit"><span class="glyphicon glyphicon-search"></span></button>
                  </span>
                  
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>

      <div class="container">
        &nbsp;
      </div>

      <div class="container">
        <!-- Table -->
        <table class="table table-striped">
          <tr>
            <th>Content</th>
            <th>Tag</th>
            <th>Note</th>
            <th>Source</th>
            <th>Date</th>
          </tr>
          <% for (i=0; i<appdata.length; i++){ %>
            <tr>
              <td> 
                <%= appdata[i].content %>
              </td>
              <td> 
                <%= appdata[i].tag %>
              </td>
              <td> 
                <%= appdata[i].note %>
              </td>
              <td> 
                <%= appdata[i].source %>
              </td>
              <td> 
                <%= appdata[i].date %>
              </td>
            </tr>
          <% }; %>

        </table>

      </div>

    </main>

    <footer>
      <% include ./partials/footer %>
    </footer>  
  </body>  
</html>
